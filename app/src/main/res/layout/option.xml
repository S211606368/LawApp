<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/optopn">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:stateListAnimator="@dimen/qb_px_5"
        android:layout_marginEnd="@dimen/qb_px_5"
        android:layout_marginTop="@dimen/qb_px_5"
        android:layout_marginBottom="@dimen/qb_px_5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >

        <TextView
            android:id="@+id/copy"
            android:layout_width="@dimen/qb_px_50"
            android:layout_height="@dimen/qb_px_30"
            android:text="复制"
            android:gravity="center"
            android:textSize="@dimen/qb_px_20"/>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>